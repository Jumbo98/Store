{
  "welcomeFile": "/project1/index.html",
  "authenticationMethod": "route",
  "routes": [
    {
      "source": "^/project1/(.*)$",
      "target": "$1",
      "destination": "html5-apps-repo-rt",
      "authenticationType": "xsuaa"
    },
    {
      "source": "^/srv/(.*)$",
      "target": "$1",
      "destination": "srv-api",
      "authenticationType": "xsuaa",
      "csrfProtection": true
    }
  ]
}